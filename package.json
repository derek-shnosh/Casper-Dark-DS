{
    "name": "casper-dark-ds",
    "description": "Clone of the default personal blogging theme for Ghost, Casper.",
    "demo": "https://shnosh.io",
    "version": "2.11.1.009",
    "engines": {
        "ghost": ">=2.0.0",
        "ghost-api": "v4"
    },
    "license": "MIT",
    "screenshots": {
        "desktop": "assets/screenshot-desktop-post-1-code.png"
    },
    "scripts": {
        "dev": "gulp",
        "zip": "gulp zip",
        "test": "gscan .",
        "pretest": "gulp build",
        "preship": "yarn test",
        "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then yarn version && git push --follow-tags; else echo \"Uncomitted changes found.\" && exit 1; fi",
        "postship": "git fetch && gulp release"
    },
    "author": {
        "name": "derek-shnosh",
        "email": "derek@shnosh.co",
        "url": "https://shnosh.io"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal",
            "Magazine"
        ]
    },
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/derek-shnosh/Casper-Dark-DS.git"
    },
    "bugs": "https://github.com/derek-shnosh/Casper-Dark-DS/issues",
    "contributors": "https://github.com/derek-shnosh/Casper-Dark-DS/graphs/contributors",
    "devDependencies": {
        "@tryghost/release-utils": "0.3.2",
        "autoprefixer": "9.6.1",
        "beeper": "1.1.1",
        "cssnano": "4.1.10",
        "gscan": "2.7.0",
        "gulp": "4.0.2",
        "gulp-livereload": "4.0.1",
        "gulp-postcss": "8.0.0",
        "gulp-uglify": "3.0.2",
        "gulp-zip": "4.2.0",
        "postcss-color-function": "4.1.0",
        "postcss-custom-properties": "8.0.11",
        "postcss-easy-import": "3.0.0",
        "pump": "3.0.0"
    },
    "config": {
        "posts_per_page": 25,
        "image_sizes": {
            "xxs": {
                "width": 30
            },
            "xs": {
                "width": 100
            },
            "s": {
                "width": 300
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1000
            },
            "xl": {
                "width": 2000
            }
        }
    },
    "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
